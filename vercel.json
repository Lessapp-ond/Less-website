{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.less-app.com"
        }
      ],
      "destination": "https://less-app.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [],
  "headers": [
    {
      "source": "/app-ads.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain"
        }
      ]
    }
  ]
}
